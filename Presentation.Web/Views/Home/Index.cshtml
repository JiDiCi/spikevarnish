<html>
<head>
    <script src="~/js/lib/jquery/jquery-3.3.1.js"></script>
</head>
<body>
    <input type="button" value="Call AJAX Service requete" onclick="callServiceRequete();" />
    <input type="button" value="Call AJAX Service presentation sans If-None-Match" onclick="callServicePresentationSansIfNoneMatch();" />
    <input type="button" value="Call AJAX Service presentation avec If-None-Match" onclick="callServicePresentationAvecIfNoneMatch();" />
</body>


<script type="text/javascript">
    function callServiceRequete() {
        $.get("http://localhost:5001/Test/GetSomething", function (data, status) {
            alert("Data: " + data + "\nStatus: " + status);
        });
    }

    function callServicePresentationAvecIfNoneMatch() {
        $.ajax({
            url: "http://10.2.211.22:6081/Home/GetSomething",
            type: 'GET',
            dataType: 'json',
            headers: {
                'Cache-Control': 'no-cache',
                'If-None-Match' : '123456'
            },
            contentType: 'application/json; charset=utf-8',
            success: function (result) {
                alert(result);
            },
            error: function (error) {

            }
        });
    }

    function callServicePresentationSansIfNoneMatch() {
        $.ajax({
            url: "http://10.2.211.22:6081/Home/GetSomething",
            type: 'GET',
            dataType: 'json',
            headers: {
                'Cache-Control': 'no-cache'
            },
            contentType: 'application/json; charset=utf-8',
            success: function (result) {
                alert(result);
            },
            error: function (error) {

            }
        });
    }
</script>




</html>